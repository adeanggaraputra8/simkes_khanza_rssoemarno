	<queryString>
		<![CDATA[select distinct a.nm_perawatan, (ifnull(b.total,0) + ifnull(c.total,0) + ifnull(c.total,0)) total FROM
(
(
select kd_jenis_prw, nm_perawatan from jns_perawatan
) as a
left JOIN
(select p.kd_jenis_prw, sum(r.biaya_rawat) total from reg_periksa k
inner join rawat_jl_drpr r on r.no_rawat = k.no_rawat
inner join jns_perawatan p on r.kd_jenis_prw = p.kd_jenis_prw
inner join reg_periksa rp on rp.no_rawat=k.no_rawat
where k.tgl_registrasi BETWEEN '2018-05-01' AND '2018-05-30' AND k.status_lanjut='ralan' and rp.kd_pj='002'
GROUP BY p.nm_perawatan
) as b on a.kd_jenis_prw = b.kd_jenis_prw
left JOIN
(
select p.kd_jenis_prw, sum(r.biaya_rawat) total from reg_periksa k
inner join rawat_jl_dr r on r.no_rawat = k.no_rawat
inner join jns_perawatan p on r.kd_jenis_prw = p.kd_jenis_prw
inner join reg_periksa rp on rp.no_rawat=k.no_rawat
where k.tgl_registrasi BETWEEN '2018-05-01' AND '2018-05-30' AND k.status_lanjut='ralan' and rp.kd_pj='002'
GROUP BY p.nm_perawatan
)as c on a.kd_jenis_prw = c.kd_jenis_prw
left JOIN
(
select p.kd_jenis_prw, sum(r.biaya_rawat) total from reg_periksa k
inner join rawat_jl_pr r on r.no_rawat = k.no_rawat
inner join jns_perawatan p on r.kd_jenis_prw = p.kd_jenis_prw
inner join reg_periksa rp on rp.no_rawat=k.no_rawat
where k.tgl_registrasi BETWEEN '2018-05-01' AND '2018-05-30' AND k.status_lanjut='ralan' and rp.kd_pj='002'
GROUP BY p.nm_perawatan
)as d on a.kd_jenis_prw = d.kd_jenis_prw
) where (ifnull(b.total,0) >0 or ifnull(c.total,0) > 0 or ifnull(c.total,0) > 0)]]>
	</queryString>
