	<queryString>
		<![CDATA[SELECT pasien.no_rkm_medis,pasien.nm_pasien,tanggal,bangsal.nm_bangsal,PLEB,diagnosa FROM data_HAIs  
INNER JOIN reg_periksa ON reg_periksa.no_rawat=data_HAIs.no_rawat  
INNER JOIN pasien ON pasien.no_rkm_medis=reg_periksa.no_rkm_medis  
INNER JOIN kamar_inap ON kamar_inap.no_rawat=reg_periksa.no_rawat  
INNER JOIN kamar ON kamar.kd_kamar=data_HAIs.kd_kamar  
INNER JOIN bangsal ON bangsal.kd_bangsal=kamar.kd_bangsal  
where data_HAIs.PLEB >0 and data_HAIs.tanggal BETWEEN '2019-09-01' AND '2019-09-30'  
GROUP BY pasien.nm_pasien order by bangsal.nm_bangsal]]>
	</queryString>
